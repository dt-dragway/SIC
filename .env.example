# SIC Ultra - Environment Configuration Template
# Copy this file to .env and fill with your actual values
# NEVER commit .env to git!

# === Database ===
POSTGRES_USER=postgres
POSTGRES_PASSWORD=YOUR_SECURE_PASSWORD_HERE
POSTGRES_DB=sic_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# === Security ===
SECRET_KEY=GENERATE_RANDOM_SECRET_KEY_MIN_32_CHARS
JWT_SECRET_KEY=GENERATE_RANDOM_JWT_SECRET_KEY_MIN_32_CHARS

# === Binance API (NEVER commit real keys!) ===
BINANCE_API_KEY=YOUR_BINANCE_API_KEY
BINANCE_API_SECRET=YOUR_BINANCE_API_SECRET
BINANCE_TESTNET=false

# === AI/LLM APIs (optional) ===
DEEPSEEK_API_KEY=
OPENAI_API_KEY=

# === Security Features ===
ENABLE_RATE_LIMIT=false  # Set to true in production
